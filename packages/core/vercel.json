{
  "rewrites": [
    {
      "source": "/composer",
      "destination": "https://elasticpath-dev-composer.vercel.app/composer"
    },
    {
      "source": "/composer/:match*",
      "destination": "https://elasticpath-dev-composer.vercel.app/composer/:match*"
    },
    {
      "source": "/",
      "destination": "https://stover.io/"
    },
    {
      "source": "/:match*",
      "destination": "https://stover.io:match*"
    }
  ]
}
